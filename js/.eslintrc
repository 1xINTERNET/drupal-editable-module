{
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": false,
    "codeFrame": true
  },
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "global-require": 0,
    "import/prefer-default-export": 0,
    "no-nested-ternary": 0
  }
}
