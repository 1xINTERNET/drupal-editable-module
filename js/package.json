{
  "name": "editable_core",
  "version": "1.0.0",
  "description": "Provides a shared redux store with entity cache and utility functions and components to decouple Drupal",
  "main": "dist/editable.js",
  "scripts": {
    "build": "npm run clean && npx run-p build:prod build:dev",
    "build:prod": "cross-env NODE_ENV=production webpack",
    "build:dev": "cross-env NODE_ENV=development webpack",
    "clean": "npx rimraf dist && npx mkdirp dist",
    "start": "webpack-dev-server",
    "lint": "eslint src",
    "format": "prettier --write './src/**/*.js'",
    "test": "jest",
    "prepublish": "npm run test && npm run build"
  },
  "author": "Martin Spencer <martin@1xinternet.de>",
  "license": "GPL-2.0+",
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-syntax-export-default-from": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.2",
    "cross-env": "^5.2.0",
    "emotion": "^9.2.9",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "expose-loader": "^0.7.5",
    "immutable-set": "^2.1.0",
    "jest": "^23.6.0",
    "lodash.get": "^4.4.2",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.3",
    "object-assign": "^4.1.1",
    "prettier": "^1.14.2",
    "promise": "^8.0.2",
    "prop-types": "^15.6.2",
    "react": "^16.5.1",
    "react-dev-utils": "^5.0.2",
    "react-dom": "^16.5.1",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-json-api": "^2.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1",
    "rimraf": "^2.6.2",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8",
    "webpack-merge": "^4.1.4",
    "whatwg-fetch": "^3.0.0"
  },
  "config": {
    "namespace": "Editable",
    "bundleName": "editable"
  },
  "dependencies": {},
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "./src/setupTests.js"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  }
}
